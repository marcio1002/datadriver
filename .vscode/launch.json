  "version": "0.2.0",
  "configurations": [
    {
      "name": "xDebug",
      "type": "php",
      "request": "launch",
      "port": 9000,
      "ignore": [
        "**/vendor/**/*.php"
      ]
    },
    {
      "name": "debug cli",
      "type": "php",
      "request": "launch",
      "program": "${file}",
      "cwd": "${fileDirname}",
      "port": 9000,
      "ignore": [
        "**/vendor/**/*.php"
      ]
    }
  ]
}